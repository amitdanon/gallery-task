;AAAA;AACA,gBAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AADA;AAAA;AAAA,uBAGA,WAAA,CAAA,sBAAA,EAAA,MAAA,CAAA,YAAA,CAHA;;AAAA;AAGA,gBAAA,GAHA;AAIA,gBAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,EAAA;AALA;AAAA;;AAAA;AAAA;AAAA;AAOA,gBAAA,MAAA,CAAA,KAAA,GAAA,sBAAA;;AAPA;AASA,gBAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAlBA;AAmBA,IAAA,YAnBA,wBAmBA,SAnBA,EAmBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,WAAA,EAAA,SAAA;AACA,WAAA,SAAA,GAAA,SAAA;AACA,WAAA,KAAA,CAAA,SAAA,CAAA,SAAA;AACA,KAvBA;AAwBA,IAAA,eAxBA,2BAwBA,MAxBA,EAwBA;AACA,UAAA,MAAA,GAAA,CAAA,IAAA,MAAA,KAAA,KAAA,MAAA,CAAA,MAAA,EAAA;AACA;AACA;;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA;AACA,WAAA,SAAA;AAAA,QAAA,KAAA,EAAA;AAAA,SAAA,MAAA;AACA,KA9BA;AA+BA,IAAA,wBA/BA,oCA+BA,cA/BA,EA+BA,KA/BA,EA+BA;AACA,aACA,cAAA,KAAA,MAAA,IAAA,KAAA,KAAA,CAAA,IACA,cAAA,KAAA,MAAA,IAAA,KAAA,KAAA,KAAA,MAAA,CAAA,MAAA,GAAA,CAFA;AAIA;AApCA;AAvBA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <form class=\"form-container\">\n      <label>\n        Search:\n        <input v-model=\"flickrParams.tags\" type=\"text\" class=\"search-input\" />\n      </label>\n      <button type=\"submit\" class=\"go-button\" @click.prevent=\"search\">\n        Search\n      </button>\n    </form>\n    <p v-if=\"error\" class=\"error-msg\">Failed to fetch data</p>\n    <div v-if=\"images.length\" v-observe-visibility=\"visibilityChanged\" class=\"list-container\">\n      <p v-if=\"loading\">Loading...</p>\n      <ul\n        v-else\n        class=\"image-card-grid scrolling-component\"\n        ref=\"infinitescrolltrigger\"\n      >\n        <image-card\n          @eventname=\"updateparent\"\n          v-for=\"(image, index) in images\"\n          :imageIndex=\"index\"\n          :key=\"image.id\"\n          :image=\"image\"\n        />\n      </ul>\n      <modal ref=\"modalName\">\n        <template v-slot:body>\n          <img\n            class=\"image-card__image\"\n            :src=\"modalData.url_n\"\n            :alt=\"modalData.title\"\n          />\n        </template>\n\n        <template v-slot:footer>\n          <div class=\"d-flex align-items-center justify-content-between\">\n            <button\n              :disabled=\"isDisableModalPagination('prev', modalData.index)\"\n              class=\"btn\"\n              @click=\"paginationModal(modalData.index - 1)\"\n            >\n              prev\n            </button>\n            <button\n              :disabled=\"isDisableModalPagination('next', modalData.index)\"\n              class=\"btn\"\n              @click=\"paginationModal(modalData.index + 1)\"\n            >\n              next\n            </button>\n          </div>\n        </template>\n      </modal>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ImageCard from './ImageCard.vue'\nimport fetchFlickr from '../api/flickr.ts'\nimport Modal from './Modal.vue'\n\nexport default {\n  name: 'Gallery',\n  components: {\n    ImageCard,\n    Modal\n  },\n  data () {\n    return {\n      error: '',\n      loading: true,\n      images: [],\n      modalData: {},\n      flickrParams: {\n        page: 1,\n        tags: 'dog',\n        perPage: 30,\n        extras: 'url_n'\n      }\n    }\n  },\n  async mounted () {\n    await this.search()\n  },\n  methods: {\n    async visibilityChanged () {\n      console.log('hhhh')\n      // if (!this.error) {\n      //   this.flickrParams.page += 1\n      //   await this.search()\n      // }\n    },\n    async search () {\n      this.loading = true\n      try {\n        const res = await fetchFlickr('flickr.photos.search', this.flickrParams)\n        this.images = res.data\n        this.error = ''\n      } catch (e) {\n        this.error = 'failed to fetch data'\n      }\n      this.loading = false\n    },\n    updateparent (modalData) {\n      console.log('modalData', modalData)\n      this.modalData = modalData\n      this.$refs.modalName.openModal()\n    },\n    paginationModal (nIndex) {\n      if (nIndex < 0 || nIndex === this.images.length) {\n        return\n      }\n      const nImage = this.images[nIndex]\n      this.modalData = { index: nIndex, ...nImage }\n    },\n    isDisableModalPagination (paginationType, index) {\n      return (\n        (paginationType === 'prev' && index === 0) ||\n        (paginationType === 'next' && index === this.images.length - 1)\n      )\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.home{\n    text-align: center;\n    background-color: #F2F2F2;\n    width:100%;\n    height: 100%;\n}\n.form-container{\n  padding: 40px;\n  font-size: 22px;\n  font-weight: bold;\n}\n.search-input{\n  padding: 5px;\n    font-size: 15px;\n    font-weight: bold;\n    outline: none;\n    border-radius: 8px;\n    border-color: gray;\n    width: 43%;\n    background-color: #F2F4FB;\n    border: 1px solid #30419B;\n    color: black;\n}\n.image-card-grid {\n  list-style: none;\n  margin: 0.5rem 0;\n  padding: 0;\n  display: flex;\n  align-items: flex-start;\n  flex-wrap: wrap;\n}\n.btn {\n  padding: 0.5rem 1rem;\n  font-size: 1rem;\n  border-radius: 20px;\n  background: transparent;\n  border: none;\n  cursor: pointer;\n}\n.go-button{\n  background: #30419b;\n  border-radius: 8px;\n  width: 100px;\n  height: 30px;\n  color: white;\n  border: none;\n  margin-left: 10px;\n  font-weight: bold;\n  font-size: 15px;\n  cursor: pointer;\n}\n.btn:disabled{\n  cursor:not-allowed;\n}\n.error-msg{\n  color:red;\n}\n.list-container{\n  width:100%;\n  height:100%;\n}\n</style>\n"],"sourceRoot":"src/components"}]}